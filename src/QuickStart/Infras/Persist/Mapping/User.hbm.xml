<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="QuickStart" namespace="QuickStart.Models">
  <class name="User" table="User" lazy="false">
    <id name="Id" type="String" length="20">
      <generator class="Radial.Persist.Nhs.TimingIdGenerator, Radial.Persist.Nhs"/>
    </id>
    <version name="Version" type="Int32" unsaved-value="0"/>
    <property name="Name" length="50" not-null="true"/>
    <property name="Email" length="50"/>
  </class>
</hibernate-mapping>