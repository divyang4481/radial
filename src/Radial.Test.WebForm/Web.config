<?xml version="1.0"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细信息，请访问
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <configSections>
    <section name="connGroup" type="Radial.Persist.Lite.Cfg.ConnectionSection, Radial.Persist.Lite"/>
  </configSections>
  <connGroup>
    <conns>
      <add name="Default" conn="Data Source=.;Initial Catalog=dfx;Integrated Security=True"/>
      <add name="phSub1" conn="Data Source=[Folder]TestDb.db3;Version=3;" type="Sqlite"/>
    </conns>
  </connGroup>
  <!--
    有关 .NET 4.5 的 web.config 更改的说明，请参见 http://go.microsoft.com/fwlink/?LinkId=235367。

    可在 <httpRuntime> 标记上设置以下特性。
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <httpRuntime maxRequestLength="40960"/>
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
</configuration>