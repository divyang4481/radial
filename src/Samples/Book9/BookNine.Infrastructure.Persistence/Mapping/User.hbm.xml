<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="BookNine.Domain" namespace="BookNine.Domain.Model">
  <class name="User">
    <id name="Id">
      <generator class="native"/>
    </id>
    <property name="Mail" not-null="true"/>
    <property name="Password" not-null="true"/>
    <property name="Salt" not-null="true"/>
    <property name="RegisterTime"/>
  </class>
</hibernate-mapping>